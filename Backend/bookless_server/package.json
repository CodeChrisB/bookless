{
  "name": "bookless_server",
  "version": "1.0.0",
  "description": "Node.js on Docker",
  "author": "Bookless Team",
  "main": ".src/index.ts",
  "scripts": {
    "dev": "ts-node-dev bookless_server/src/server.ts",
    "start": "ts-node src/server.ts",
    "build": "tsc -p .",
    "docker":"docker run -d -p 5432:5432 -v ./pgdata:/var/lib/databases/postgresql/data -e 'POSTGRES_USER=user' -e 'POSTGRES_PASSWORD=pass' -e 'POSTGRES_DB=booklessdb' postgres"
    
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwt-simple": "^0.5.33",
    "@types/node": "^14.14.10",
    "@types/pg": "^7.14.7",
    "express": "^4.17.1",
    "nodemon": "^2.0.6",
    "pg": "^8.5.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "env": "0.0.2",
    "ini": ">=1.3.6",
    "inversify": "^5.0.5",
    "inversify-express-utils": "^6.3.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "pg-promise": "^10.9.2",
    "reflect-metadata": "^0.1.13",
    "ts-postgres": "^1.1.3",
    "winston": "^3.3.3"
  }
}
